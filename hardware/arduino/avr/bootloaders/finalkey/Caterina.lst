 154:Caterina.c    **** /** Configures all hardware required for the bootloader. */
 155:Caterina.c    **** void SetupHardware(void)
 156:Caterina.c    **** {
 157:Caterina.c    **** 	/* Disable watchdog if enabled by bootloader/fuses */
 158:Caterina.c    **** 	MCUSR &= ~(1 << WDRF);
 159:Caterina.c    **** 	wdt_disable();
 160:Caterina.c    **** 
 161:Caterina.c    **** 	/* Disable clock division */
 162:Caterina.c    **** 	clock_prescale_set(clock_div_1);
 163:Caterina.c    **** 
 164:Caterina.c    **** 	/* Relocate the interrupt vector table to the bootloader section */
 165:Caterina.c    **** 	MCUCR = (1 << IVCE);
 166:Caterina.c    **** 	MCUCR = (1 << IVSEL);
 167:Caterina.c    **** 	
 168:Caterina.c    **** 	LED_SETUP();
 169:Caterina.c    **** 	CPU_PRESCALE(0); 
 170:Caterina.c    **** 	L_LED_OFF();
 171:Caterina.c    **** 	TX_LED_OFF();
 172:Caterina.c    **** 	RX_LED_OFF();
 173:Caterina.c    **** 	
 174:Caterina.c    **** 	/* Initialize TIMER1 to handle bootloader timeout and LED tasks.  
 175:Caterina.c    **** 	 * With 16 MHz clock and 1/64 prescaler, timer 1 is clocked at 250 kHz
 176:Caterina.c    **** 	 * Our chosen compare match generates an interrupt every 1 ms.
 177:Caterina.c    **** 	 * This interrupt is disabled selectively when doing memory reading, erasing,
 178:Caterina.c    **** 	 * or writing since SPM has tight timing requirements.
 179:Caterina.c    **** 	 */ 
 180:Caterina.c    **** 	OCR1AH = 0;
 181:Caterina.c    **** 	OCR1AL = 250;
 182:Caterina.c    **** 	TIMSK1 = (1 << OCIE1A);					// enable timer 1 output compare A match interrupt
 183:Caterina.c    **** 	TCCR1B = ((1 << CS11) | (1 << CS10));	// 1/64 prescaler on timer 1 input
 184:Caterina.c    **** 
 185:Caterina.c    **** 	/* Initialize USB Subsystem */
 186:Caterina.c    **** 	USB_Init();
 187:Caterina.c    **** }
