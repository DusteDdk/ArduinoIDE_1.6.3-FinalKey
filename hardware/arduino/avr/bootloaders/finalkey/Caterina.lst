 257:Caterina.c    **** #if !defined(NO_BLOCK_SUPPORT)
 258:Caterina.c    **** /** Reads or writes a block of EEPROM or FLASH memory to or from the appropriate CDC data endpoint,
 259:Caterina.c    ****  *  on the AVR910 protocol command issued.
 260:Caterina.c    ****  *
 261:Caterina.c    ****  *  \param[in] Command  Single character AVR910 protocol command indicating what memory operation t
 262:Caterina.c    ****  */
 263:Caterina.c    **** static void ReadWriteMemoryBlock(const uint8_t Command)
 264:Caterina.c    **** {
 265:Caterina.c    **** 	uint16_t BlockSize;
 266:Caterina.c    **** 	char     MemoryType;
 267:Caterina.c    **** 
 268:Caterina.c    **** 	bool     HighByte = false;
 269:Caterina.c    **** 	uint8_t  LowByte  = 0;
 270:Caterina.c    **** 
 271:Caterina.c    **** 	BlockSize  = (FetchNextCommandByte() << 8);
 272:Caterina.c    **** 	BlockSize |=  FetchNextCommandByte();
 273:Caterina.c    **** 
 274:Caterina.c    **** 	MemoryType =  FetchNextCommandByte();
 275:Caterina.c    **** 
 276:Caterina.c    **** 	if ((MemoryType != 'E') && (MemoryType != 'F'))
 277:Caterina.c    **** 	{
 278:Caterina.c    **** 		/* Send error byte back to the host */
 279:Caterina.c    **** 		WriteNextResponseByte('?');
 280:Caterina.c    **** 
 281:Caterina.c    **** 		return;
 282:Caterina.c    **** 	}
 283:Caterina.c    **** 
 284:Caterina.c    **** 	/* Disable timer 1 interrupt - can't afford to process nonessential interrupts
 285:Caterina.c    **** 	 * while doing SPM tasks */
 286:Caterina.c    **** 	TIMSK1 = 0;
 287:Caterina.c    **** 
 288:Caterina.c    **** 	/* Check if command is to read memory */
 289:Caterina.c    **** 	if (Command == 'g')
 290:Caterina.c    **** 	{		
 291:Caterina.c    **** 		/* Re-enable RWW section */
 292:Caterina.c    **** 		boot_rww_enable();
 293:Caterina.c    **** 
 294:Caterina.c    **** 		while (BlockSize--)
 295:Caterina.c    **** 		{
 296:Caterina.c    **** 			if (MemoryType == 'F')
 297:Caterina.c    **** 			{
 298:Caterina.c    **** 				/* Read the next FLASH byte from the current FLASH page */
 299:Caterina.c    **** 				#if (FLASHEND > 0xFFFF)
 300:Caterina.c    **** 				WriteNextResponseByte(pgm_read_byte_far(CurrAddress | HighByte));
 301:Caterina.c    **** 				#else
 302:Caterina.c    **** 				WriteNextResponseByte(pgm_read_byte(CurrAddress | HighByte));
 303:Caterina.c    **** 				#endif
 304:Caterina.c    **** 
 305:Caterina.c    **** 				/* If both bytes in current word have been read, increment the address counter */
 306:Caterina.c    **** 				if (HighByte)
 307:Caterina.c    **** 				  CurrAddress += 2;
 308:Caterina.c    **** 
 309:Caterina.c    **** 				HighByte = !HighByte;
 310:Caterina.c    **** 			}
 311:Caterina.c    **** 			else
 312:Caterina.c    **** 			{
 313:Caterina.c    **** 				/* Read the next EEPROM byte into the endpoint */
 314:Caterina.c    **** 				WriteNextResponseByte(eeprom_read_byte((uint8_t*)(intptr_t)(CurrAddress >> 1)));
 315:Caterina.c    **** 
 316:Caterina.c    **** 				/* Increment the address counter after use */
 317:Caterina.c    **** 				CurrAddress += 2;
 318:Caterina.c    **** 			}
 319:Caterina.c    **** 		}
 320:Caterina.c    **** 	}
 321:Caterina.c    **** 	else
 322:Caterina.c    **** 	{
 323:Caterina.c    **** 		uint32_t PageStartAddress = CurrAddress;
 324:Caterina.c    **** 	
 325:Caterina.c    **** 		if (MemoryType == 'F')
 326:Caterina.c    **** 		{
 327:Caterina.c    **** 			boot_page_erase(PageStartAddress);
 328:Caterina.c    **** 			boot_spm_busy_wait();
 329:Caterina.c    **** 		}
 330:Caterina.c    **** 
 331:Caterina.c    **** 		while (BlockSize--)
 332:Caterina.c    **** 		{
 333:Caterina.c    **** 			if (MemoryType == 'F')
 334:Caterina.c    **** 			{
 335:Caterina.c    **** 				/* If both bytes in current word have been written, increment the address counter */
 336:Caterina.c    **** 				if (HighByte)
 337:Caterina.c    **** 				{
 338:Caterina.c    **** 					/* Write the next FLASH word to the current FLASH page */
 339:Caterina.c    **** 					boot_page_fill(CurrAddress, ((FetchNextCommandByte() << 8) | LowByte));
 340:Caterina.c    **** 
 341:Caterina.c    **** 					/* Increment the address counter after use */
 342:Caterina.c    **** 					CurrAddress += 2;
 343:Caterina.c    **** 				}
 344:Caterina.c    **** 				else
 345:Caterina.c    **** 				{
 346:Caterina.c    **** 					LowByte = FetchNextCommandByte();
 347:Caterina.c    **** 				}
 348:Caterina.c    **** 				
 349:Caterina.c    **** 				HighByte = !HighByte;
 350:Caterina.c    **** 			}
 351:Caterina.c    **** 			else
 352:Caterina.c    **** 			{
 353:Caterina.c    **** 				/* Write the next EEPROM byte from the endpoint */
 354:Caterina.c    **** 				eeprom_write_byte((uint8_t*)((intptr_t)(CurrAddress >> 1)), FetchNextCommandByte());
 355:Caterina.c    **** 
 356:Caterina.c    **** 				/* Increment the address counter after use */
 357:Caterina.c    **** 				CurrAddress += 2;
 358:Caterina.c    **** 			}
 359:Caterina.c    **** 		}
 360:Caterina.c    **** 
 361:Caterina.c    **** 		/* If in FLASH programming mode, commit the page after writing */
 362:Caterina.c    **** 		if (MemoryType == 'F')
 363:Caterina.c    **** 		{
 364:Caterina.c    **** 			/* Commit the flash page to memory */
 365:Caterina.c    **** 			boot_page_write(PageStartAddress);
 366:Caterina.c    **** 
 367:Caterina.c    **** 			/* Wait until write operation has completed */
 368:Caterina.c    **** 			boot_spm_busy_wait();
 369:Caterina.c    **** 		}
 370:Caterina.c    **** 
 371:Caterina.c    **** 		/* Send response byte back to the host */
 372:Caterina.c    **** 		WriteNextResponseByte('\r');
 373:Caterina.c    **** 	}
 374:Caterina.c    **** 
 375:Caterina.c    **** 	/* Re-enable timer 1 interrupt disabled earlier in this routine */	
 376:Caterina.c    **** 	TIMSK1 = (1 << OCIE1A);
 377:Caterina.c    **** }
 378:Caterina.c    **** #endif
