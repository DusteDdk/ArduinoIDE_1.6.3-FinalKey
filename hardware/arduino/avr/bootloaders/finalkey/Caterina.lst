 216:Caterina.c    **** /** Event handler for the USB_ControlRequest event. This is used to catch and process control reque
 217:Caterina.c    ****  *  the device from the USB host before passing along unhandled control requests to the library for
 218:Caterina.c    ****  *  internally.
 219:Caterina.c    ****  */
 220:Caterina.c    **** void EVENT_USB_Device_ControlRequest(void)
 221:Caterina.c    **** {
 222:Caterina.c    **** 	/* Ignore any requests that aren't directed to the CDC interface */
 223:Caterina.c    **** 	if ((USB_ControlRequest.bmRequestType & (CONTROL_REQTYPE_TYPE | CONTROL_REQTYPE_RECIPIENT)) !=
 224:Caterina.c    **** 	    (REQTYPE_CLASS | REQREC_INTERFACE))
 225:Caterina.c    **** 	{
 226:Caterina.c    **** 		return;
 227:Caterina.c    **** 	}
 228:Caterina.c    **** 
 229:Caterina.c    **** 	/* Process CDC specific control requests */
 230:Caterina.c    **** 	switch (USB_ControlRequest.bRequest)
 231:Caterina.c    **** 	{
 232:Caterina.c    **** 		case CDC_REQ_GetLineEncoding:
 233:Caterina.c    **** 			if (USB_ControlRequest.bmRequestType == (REQDIR_DEVICETOHOST | REQTYPE_CLASS | REQREC_INTERFACE)
 234:Caterina.c    **** 			{
 235:Caterina.c    **** 				Endpoint_ClearSETUP();
 236:Caterina.c    **** 
 237:Caterina.c    **** 				/* Write the line coding data to the control endpoint */
 238:Caterina.c    **** 				Endpoint_Write_Control_Stream_LE(&LineEncoding, sizeof(CDC_LineEncoding_t));
 239:Caterina.c    **** 				Endpoint_ClearOUT();
 240:Caterina.c    **** 			}
 241:Caterina.c    **** 
 242:Caterina.c    **** 			break;
 243:Caterina.c    **** 		case CDC_REQ_SetLineEncoding:
 244:Caterina.c    **** 			if (USB_ControlRequest.bmRequestType == (REQDIR_HOSTTODEVICE | REQTYPE_CLASS | REQREC_INTERFACE)
 245:Caterina.c    **** 			{
 246:Caterina.c    **** 				Endpoint_ClearSETUP();
 247:Caterina.c    **** 
 248:Caterina.c    **** 				/* Read the line coding data in from the host into the global struct */
 249:Caterina.c    **** 				Endpoint_Read_Control_Stream_LE(&LineEncoding, sizeof(CDC_LineEncoding_t));
 250:Caterina.c    **** 				Endpoint_ClearIN();
 251:Caterina.c    **** 			}
 252:Caterina.c    **** 
 253:Caterina.c    **** 			break;
 254:Caterina.c    **** 	}
 255:Caterina.c    **** }
