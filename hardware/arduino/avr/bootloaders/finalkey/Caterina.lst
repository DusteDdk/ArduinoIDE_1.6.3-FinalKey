  88:Caterina.c    **** /** Main program entry point. This routine configures the hardware required by the bootloader, then
  89:Caterina.c    ****  *  runs the bootloader processing routine until it times out or is instructed to exit.
  90:Caterina.c    ****  */
  91:Caterina.c    **** int main(void)
  92:Caterina.c    **** {
  93:Caterina.c    **** 	//Set pe6 (arduino 7) to output
  94:Caterina.c    **** 	DDRE |= (1<<6);
  95:Caterina.c    **** 	//set 7 to 0
  96:Caterina.c    **** 	PORTE &= ~(1<<6);
  97:Caterina.c    **** 	//Set pb5 (arduino 9) to input
  98:Caterina.c    **** 	DDRB &= ~(1<<5);	
  99:Caterina.c    **** 	//set 9 to 1 to pull it up
 100:Caterina.c    **** 	PORTB |= (1<<5);
 101:Caterina.c    **** 	
 102:Caterina.c    **** 	_delay_ms(100);
 103:Caterina.c    **** 	  
 104:Caterina.c    **** 	
 105:Caterina.c    **** 	int countDown = 10000;
 106:Caterina.c    **** 	DDRB |= (1<<6);
 107:Caterina.c    **** 	while( !(PINB&(1<<5)) )
 108:Caterina.c    **** 	{
 109:Caterina.c    **** 	  PORTB |= (1<<6);
 110:Caterina.c    **** 	  _delay_ms(40);
 111:Caterina.c    **** 	  
 112:Caterina.c    **** 	  PORTB &= ~(1<<6);
 113:Caterina.c    **** 	  _delay_ms(40);
 114:Caterina.c    **** 
 115:Caterina.c    **** 	  countDown-=80;
 116:Caterina.c    **** 	  while( countDown == 0 )
 117:Caterina.c    **** 	  {
 118:Caterina.c    **** 	    *bootKeyPtr = 0;
 119:Caterina.c    **** 	    wdt_disable();
 120:Caterina.c    **** 	    MCUSR = 0;							// clear all reset flags	
 121:Caterina.c    **** 	    SetupHardware();
 122:Caterina.c    **** 	    sei();
 123:Caterina.c    **** 
 124:Caterina.c    **** 	    while (1)
 125:Caterina.c    **** 	    {
 126:Caterina.c    **** 		    CDC_Task();
 127:Caterina.c    **** 		    USB_USBTask();
 128:Caterina.c    **** 	    }
 129:Caterina.c    **** 	  }
 130:Caterina.c    **** 	}
 131:Caterina.c    **** 	PORTB |= (1<<6);
 132:Caterina.c    **** 
 133:Caterina.c    **** 	
 134:Caterina.c    **** 	
 135:Caterina.c    **** 
 136:Caterina.c    **** 	cli();
 137:Caterina.c    **** 	
 138:Caterina.c    **** 	
 139:Caterina.c    **** 	TIMSK1 = 0;
 140:Caterina.c    **** 	TCCR1B = 0;
 141:Caterina.c    **** 	TCNT1H = 0;		// 16-bit write to TCNT1 requires high byte be written first
 142:Caterina.c    **** 	TCNT1L = 0;
 143:Caterina.c    **** 	
 144:Caterina.c    **** 	MCUCR = (1 << IVCE);
 145:Caterina.c    **** 	MCUCR = 0;
 146:Caterina.c    **** 
 147:Caterina.c    **** /*	L_LED_OFF();
 148:Caterina.c    **** 	TX_LED_OFF();
 149:Caterina.c    **** 	RX_LED_OFF();
 150:Caterina.c    **** 	*/
 151:Caterina.c    **** 	__asm__ volatile("jmp 0x0000");
 152:Caterina.c    **** }
