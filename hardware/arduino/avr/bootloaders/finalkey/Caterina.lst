  73:Caterina.c    **** /*	Breathing animation on L LED indicates bootloader is running */
  74:Caterina.c    **** uint16_t LLEDPulse;
  75:Caterina.c    **** void LEDPulse(void)
  76:Caterina.c    **** {
  77:Caterina.c    **** 	LLEDPulse++;
  78:Caterina.c    **** 	uint8_t p = LLEDPulse >> 8;
  79:Caterina.c    **** 	if (p > 127)
  80:Caterina.c    **** 		p = 254-p;
  81:Caterina.c    **** 	p += p;
  82:Caterina.c    **** 	if (((uint8_t)LLEDPulse) > p)
  83:Caterina.c    **** 		L_LED_OFF();
  84:Caterina.c    **** 	else
  85:Caterina.c    **** 		L_LED_ON();
  86:Caterina.c    **** }
